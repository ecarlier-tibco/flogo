{
  "name": "pushover",
  "type": "flogo:activity", 
  "ref": "github.com/ecarlier-tibco/flogo/activity/pushover",
  "version": "0.0.2",
  "title": "PushOver Notification Activity",
  "description": "Simple activity to send notifications to devices across PushOver Service",
  "author": "Eric Carlier <ecarlier@tibco.com>",
